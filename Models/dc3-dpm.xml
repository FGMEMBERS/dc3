<?xml version="1.0"?>

<PropertyList>

 <path>dc3-dpm.ac</path>

 <!--
   Note that the x and y axes are switched in the following, because
   of the different model orientation.
 -->

 <animation>
  <type>spin</type>
  <object-name>LeftPropeller</object-name>
  <property>/engines/engine[0]/rpm</property>
  <center>
   <x-m>0</x-m>
   <y-m>-2.9</y-m>
   <z-m>-1.55</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>RightPropeller</object-name>
  <property>/engines/engine[1]/rpm</property>
  <center>
   <x-m>0</x-m>
   <y-m>2.9</y-m>
   <z-m>-1.55</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>LeftWheel</object-name>
  <object-name>LeftStrut</object-name>
  <property>/controls/gear-down</property>
  <offset>-1</offset>
  <factor>-35</factor>
  <center>
   <x-m>3.8</x-m>
   <y-m>-2.9</y-m>
   <z-m>-1.8</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>LeftStrut</object-name>
  <property>/controls/gear-down</property>
  <offset>-1</offset>
  <factor>-35</factor>
  <center>
   <x-m>2.05</x-m>
   <y-m>-2.9</y-m>
   <z-m>-3.25</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RightWheel</object-name>
  <object-name>RightStrut</object-name>
  <property>/controls/gear-down</property>
  <offset>-1</offset>
  <factor>-35</factor>
  <center>
   <x-m>3.8</x-m>
   <y-m>2.9</y-m>
   <z-m>-1.8</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RightStrut</object-name>
  <property>/controls/gear-down</property>
  <offset>-1</offset>
  <factor>-35</factor>
  <center>
   <x-m>2.05</x-m>
   <y-m>2.9</y-m>
   <z-m>-3.25</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>