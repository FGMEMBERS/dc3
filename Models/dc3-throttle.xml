<?xml version="1.0"?>
<PropertyList>

 <path>dc3-throttle.ac</path>

<animation>
  <type>rotate</type>
  <object-name>Throttle</object-name>
  <property>/controls/engines/engine[0]/throttle</property>
  <factor>55</factor>

  <center>
   <x-m>0.109</x-m>
   <y-m>0.8</y-m>
   <z-m>0.128</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


