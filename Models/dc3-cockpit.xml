    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>30</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.461231</x-m>
   <y-m>0.152494</y-m>
   <z-m>-0.579749</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>FlapsLever</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>50</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.460043</x-m>
   <y-m>0.152494</y-m>
   <z-m>-0.415003</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
